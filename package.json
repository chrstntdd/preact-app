    "koa-static": "^5.0.0",
    "preact": "^10.0.4",
    "preact-render-to-string": "^5.1.1",
    "terser": "^4.3.9",
    "ua-parser-js": "^0.7.20",
    "unistore": "^3.5.0",
    "wouter-preact": "^2.3.1"
  },
